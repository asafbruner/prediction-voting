generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Question {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  options     String   // JSON string
  votes       String   // JSON string
  order       Int
  createdAt   DateTime @default(now())
}